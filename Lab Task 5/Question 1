#include <stdio.h>

int stack[10];
int top = -1;

void push(int x) {
    if (top==9) {
        printf("Stack is full\n");
    } else {
        top++;
        stack[top] = x;
        printf("%d pushed into stack\n", x);
    }
}

void pop() {
    if (top==-1) {
        printf("Stack is empty\n");
    } else {
        printf("%d popped from stack\n", stack[top]);
        top--;
    }
}

void display() {
    if (top==-1) {
        printf("Stack is empty\n");
    } else {
        printf("Stack elements: ");
        for (int i=0; i<=top; i++) {
            printf("%d ", stack[i]);
        }
        printf("\n");
    }
}

int main() {
    push(10);
    push(20);
    push(30);
    display();
    pop();
    display();
    return 0;
}
